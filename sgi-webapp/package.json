{
  "name": "sgi-webapp",
  "version": "0.0.1-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "node ./generate-config.js && ng serve --host 0.0.0.0 --allowedHosts 0.0.0.0 --disableHostCheck",
    "build": "node ./generate-config.js && ng build",
    "test": "node ./generate-config.js && ng test",
    "test-ci": "node ./generate-config.js && ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "node ./generate-config.js && ng lint",
    "e2e": "node ./generate-config.js && ng e2e",
    "e2e-ci": "node ./generate-config.js && ng e2e --webdriver-update false --protractor-config=e2e/protractor-ci.conf.js",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --key-as-default-value --format json --format-indentation '  '",
    "webdriver-update": "webdriver-manager update --versions.chrome $(google-chrome --version | cut -d ' ' -f 3)"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^2.0.4",
    "@angular-material-components/moment-adapter": "^2.0.2",
    "@angular/animations": "~9.1.10",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.10",
    "@angular/compiler": "~9.1.10",
    "@angular/core": "~9.1.10",
    "@angular/flex-layout": "~9.0.0-beta.29",
    "@angular/forms": "~9.1.10",
    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "^9.1.10",
    "@angular/platform-browser": "~9.1.10",
    "@angular/platform-browser-dynamic": "~9.1.10",
    "@angular/router": "~9.1.10",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-formly/core": "^5.0.0",
    "@ngx-formly/material": "^5.0.0",
    "@ngx-formly/schematics": "^5.9.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@sgi/framework": "0.0.1",
    "fontsource-poppins": "^3.0.3",
    "keycloak-js": "11.0.0",
    "material-icons": "^0.3.1",
    "moment": "^2.27.0",
    "ngx-logger": "^4.1.9",
    "ngx-translate-testing": "^4.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.10",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "handlebars": "^4.7.6",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "moment-timezone": "^0.5.31",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
