{
  "name": "sgi-webapp",
  "version": "0.12.0-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng build sgi-framework-angular && node ./generate-config.js && NODE_OPTIONS=--max-old-space-size=4096 ng serve sgi-webapp --disable-host-check",
    "build": "ng build sgi-framework-angular && node ./generate-config.js && NODE_OPTIONS=--max-old-space-size=8192 ng build sgi-webapp",
    "build-prod": "ng build --prod sgi-framework-angular && node ./generate-config.js && ng build sgi-webapp --prod",
    "test-framework": "ng test sgi-framework-angular --browsers=ChromeHeadlessCI",
    "test": "ng build sgi-framework-angular && node ./generate-config.js && NODE_OPTIONS=--max-old-space-size=4096 ng test sgi-webapp",
    "test-wayland": "ng build sgi-framework-angular && node ./generate-config.js && NODE_OPTIONS=--max-old-space-size=4096 ng test sgi-webapp --browsers=ChromeWayland",
    "test-ci": "ng build sgi-framework-angular && node ./generate-config.js && ng test sgi-webapp --watch=false --browsers=ChromeHeadlessCI",
    "lint": "node ./generate-config.js && ng lint",
    "e2e": "node ./generate-config.js && ng e2e",
    "e2e-ci": "node ./generate-config.js && ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --key-as-default-value --format json --format-indentation '  ' && node ./extract-aditional-translations.js",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@herculesproject/datetime-picker": "^5.1.0-sgi-SNAPSHOT",
    "@angular/animations": "~11.1.1",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.1.1",
    "@angular/language-service": "~11.1.1",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ckeditor/ckeditor5-angular": "^4.0.0",
    "@ngx-formly/core": "^5.10.13",
    "@ngx-formly/material": "^5.10.13",
    "@ngx-formly/schematics": "^5.10.13",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "ckeditor5-custom-build": "file:ckeditor5",
    "fontsource-poppins": "^3.0.3",
    "keycloak-js": "11.0.0",
    "luxon": "^1.26.0",
    "mat-timepicker": "^5.1.4",
    "material-icons": "^1.10.7",
    "messageformat": "^2.3.0",
    "ngx-cookie-service": "^11.0.2",
    "ngx-logger": "^4.1.9",
    "ngx-translate-messageformat-compiler": "^4.9.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.2",
    "@angular/cli": "~11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/luxon": "^1.26.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "handlebars": "^4.7.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.1.2",
    "ngx-translate-testing": "^5.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
