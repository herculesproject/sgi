{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@env": [
        "src/environments/environment"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@material/*": [
        "src/app/material-design/*"
      ],
      "@block/*": [
        "src/app/block/*"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@formly-forms/*": [
        "src/app/formly-forms/*"
      ],
      "@herculesproject/framework": [
        "dist/@herculesproject/framework"
      ],
      "@herculesproject/framework/*": [
        "dist/@herculesproject/framework/*"
      ],
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}