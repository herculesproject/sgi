<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:pro="http://www.liquibase.org/xml/ns/pro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.8.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
  <property name="schemaPrefix" value="" />

  <changeSet context="!test" author="master" id="0000000000003-param">
    <!-- ETI Informe de Seguimiento Final Pendiente -->
    <insert tableName="param">
      <column name="id" valueSequenceNext="param_seq" />
      <column name="name" value="ETI_COM_INF_SEG_FIN_DATA" />
      <column name="description" value="JSON con formato: {&quot;nombre de investigacion&quot;&quot;genero de comite&quot;,&quot;referencia de memoria&quot;,&quot;tipo de actividad&quot;,&quot;titulo solicitud evaluacion&quot;,&quot;enlace de aplicacion&quot;}" />
      <column name="type" value="JSON" />
      <column name="created_by" value="master" />
      <column name="creation_date" valueDate="current_timestamp()" />
      <column name="last_modified_by" value="master" />
      <column name="last_modified_date" valueDate="current_timestamp()" />
    </insert>
  </changeSet>

  <changeSet context="!test" author="master" id="0000000000003-subject_tpl">
    <!-- Informe de Seguimiento Final Pendiente -->
    <insert tableName="subject_tpl">
      <column name="id" valueSequenceNext="subject_tpl_seq" />
      <column name="name" value="ETI_COM_INF_SEG_FIN_SUBJECT" />
      <column name="description" value="Plantilla de asunto para el envío de comunicados de seguimiento final pendiente" />
      <column name="tpl" value=" " />
    </insert>
    <update tableName="subject_tpl">
      <column name="tpl" valueClobFile="clob/0000000000003/eti-com-informe/ETI_COM_INF_SEG_FIN_SUBJECT.ftl" />
      <where>name='ETI_COM_INF_SEG_FIN_SUBJECT'</where>
    </update>
  </changeSet>

  <changeSet context="!test" author="master" id="0000000000003-subjecttpl_param">
    <!-- ETI Informe de Seguimiento Final Pendiente -->
    <insert tableName="subjecttpl_param">
      <column name="subjecttpl_id" valueComputed="(select id from ${schemaPrefix}subject_tpl where name='ETI_COM_INF_SEG_FIN_SUBJECT')" />
      <column name="param_id" valueComputed="(select id from ${schemaPrefix}param where name='ETI_COM_INF_SEG_FIN_DATA')" />
    </insert>
  </changeSet>

  <changeSet context="!test" author="master" id="0000000000003-content_tpl">
