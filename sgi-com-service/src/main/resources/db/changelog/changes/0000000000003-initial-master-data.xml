    <update tableName="content_tpl">
      <column name="tpl_text" valueClobFile="clob/0000000000003/eti-com-informe/ETI_COM_INF_SEG_FIN_CONTENT_text.ftl" />
      <where>name='ETI_COM_INF_SEG_FIN_CONTENT'</where>
    </update>
    <update tableName="content_tpl">
      <column name="tpl_html" valueClobFile="clob/0000000000003/eti-com-informe/ETI_COM_INF_SEG_FIN_CONTENT_html.ftl" />
      <where>name='ETI_COM_INF_SEG_FIN_CONTENT'</where>
    </update>
  </changeSet>

  <changeSet context="!test" author="master" id="0000000000003-contenttpl_param">
    <!-- ETI Informe de Seguimiento Final Pendiente -->
    <insert tableName="contenttpl_param">
      <column name="contenttpl_id" valueComputed="(select id from ${schemaPrefix}content_tpl where name='ETI_COM_INF_SEG_FIN_CONTENT')" />
