  <changeSet context="!test" author="master" id="00.03.00-resources-update-oracle"
    dbms="oracle">
    <!-- web-header-logo-feder -->
    <update tableName="resources">
      <column name="value" valueBlobFile="blob/web/web-header-logo-feder.png" />
      <where>:name=:value AND dbms_lob.compare(value, default_value) = 0</where>
      <whereParams>
        <param name="name" value="web-header-logo-feder" />
      </whereParams>
    </update>

    <update tableName="resources">
      <column name="default_value" valueBlobFile="blob/web/web-header-logo-feder.png" />
      <where>:name=:value</where>
      <whereParams>
        <param name="name" value="web-header-logo-feder" />
