[
  {
    "key": "ap4_5_1",
    "wrappers": [
      "title-div"
    ],
    "templateOptions": {
      "expressionLock": "formState.memoria.tipoMemoria.id === 3",
      "label": "4.5.1 Result of the genetic study"
    },
    "hideExpression": "!formState.mainModel.ap4_5.estudiosGeneticosRadio || formState.mainModel.ap4_5.estudiosGeneticosRadio === 'no'",
    "fieldGroup": [
      {
        "template": "<p>Can any information relevant to the life or health of the participating subject be obtained?</p>"
      },
      {
        "key": "resultadoEstudioGeneticoRadio",
        "type": "radio",
        "templateOptions": {
          "required": true,
          "label": "Choose an option:",
          "options": [
            {
              "value": "no",
              "label": "No"
            },
            {
              "value": "si",
              "label": "Yes"
            }
          ]
        }
      },
      {
        "template": "<p>If yes, specify</p>",
        "hideExpression": "!model.resultadoEstudioGeneticoRadio || model.resultadoEstudioGeneticoRadio === 'no'"
      },
      {
        "key": "especificarResultadoEstudioGenetico",
        "type": "textarea",
        "hideExpression": "!model.resultadoEstudioGeneticoRadio || model.resultadoEstudioGeneticoRadio === 'no'",
        "templateOptions": {
          "label": "Please specify",
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      }
    ]
  }
]