          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      },
      {
        "key": "especificarProcedentesBiobanco",
        "type": "i18n-textarea",
        "hideExpression": "!model.origenMuestrasCheck || !model.origenMuestrasCheck.some(origen => origen === 'procedentesBiobanco')",
        "templateOptions": {
          "label": "Biobancotik lortu dira: zehaztu.",
          "placeholder": "Biobancotik lortu dira: zehaztu.",
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
