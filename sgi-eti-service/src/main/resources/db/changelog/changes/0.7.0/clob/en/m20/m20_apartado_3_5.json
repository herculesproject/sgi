[
  {
    "key": "ap3_5",
    "wrappers": [
      "expansion-panel"
    ],
    "templateOptions": {
      "label": "3.5 Location where animal procedures/interventions will be performed"
    },
    "fieldGroup": [
      {
        "template": "<p>Please specify the place where the animals will be kept during the experimentation process.</p>"
      },
      {
        "wrappers": [
          "info-div"
        ],
        "template": "<p>The procedures/interventions will be carried out in authorised user centres, unless authorised by the competent agency, after scientific justification of the need or convenience for them to be carried out outside of such centres (<a href='https://www.um.es/web/comision-etica-investigacion/comision-etica/ceea' target='_blank'>Art. 25-4 Royal Decree 53/13</a>)</p>"
      },
      {
        "key": "centroUsuarioUniversidadCheckPrincipal",
        "type": "checkbox",
        "templateOptions": {
          "label": "University user centre",
          "indeterminate": false
        }
      },
      {
        "key": "centroUsuarioUniversidadCheck",
        "type": "multicheckbox",
        "hideExpression": "!model.centroUsuarioUniversidadCheckPrincipal",
        "templateOptions": {
          "label": "Choose",
          "required": true,
          "type": "array",
          "options": [
            {
              "value": "animalarioCampusEspinardo",
              "label": "Espinardo Campus Animal Facility"
            },
            {
              "value": "animalarioCampusCienciasSalud",
              "label": "Health Sciences Campus Animal Facility (CEIB)"
            },
            {
              "value": "otros",
              "label": "Others (specify)"
            }
          ]
        }
      },
      {
        "key": "especificarCentroUniversidad",
        "type": "textarea",
        "hideExpression": "!(model.centroUsuarioUniversidadCheckPrincipal && model.centroUsuarioUniversidadCheck && model.centroUsuarioUniversidadCheck.find((c)=> c === 'otros'))",
        "templateOptions": {
          "label": "Please specify",
          "disabled": false,
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      },
      {
        "key": "otroCentroRegistradoCheck",
        "type": "checkbox",
        "templateOptions": {
          "label": "Another user registered centre.",
          "indeterminate": false
        }
      },
      {
        "key": "especificarCentroRegistrado",
        "type": "textarea",
        "hideExpression": "!model.otroCentroRegistradoCheck",
        "templateOptions": {
          "label": "Please specify",
          "disabled": false,
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      },
      {
        "key": "numeroRegistro",
        "type": "textarea",
        "hideExpression": "!model.otroCentroRegistradoCheck",
        "templateOptions": {
          "label": "Registration no.",
          "disabled": false,
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      },
      {
        "key": "otroCentroNoRegistradoCheck",
        "type": "checkbox",
        "templateOptions": {
          "label": "Another unregistered centre or place.",
          "indeterminate": false
        }
      },
      {
        "key": "especificarCentroNoRegistrado",
        "type": "textarea",
        "hideExpression": "!model.otroCentroNoRegistradoCheck",
        "templateOptions": {
          "label": "Please specify",
          "disabled": false,
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      },
      {
        "key": "justificar",
        "type": "textarea",
        "hideExpression": "!model.otroCentroNoRegistradoCheck",
        "templateOptions": {
          "label": "Justify",
          "disabled": false,
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      },
      {
        "wrappers": [
          "info-div"
        ],
        "template": "<p>For more information on places in the University User Centre, consult the <a href='https://www.um.es/web/comision-etica-investigacion/comision-etica/ceea' target='_blank'>University Committee website</a>.</p>"
      }
    ]
  }
]