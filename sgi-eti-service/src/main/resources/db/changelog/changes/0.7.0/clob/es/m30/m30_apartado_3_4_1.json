[
  {
    "key": "ap3_4_1",
    "wrappers": [
      "title-div"
    ],
    "templateOptions": {
      "label": "3.4.1 Nahigabeko edo ustekabeko esposizioa"
    },
    "fieldGroup": [
      {
        "template": "<p>Jarduera honetan, agente biologikoekiko nahigabeko edo ustekabeko esposizioa ekar dezaketen zereginak egiten dira?</p>"
      },
      {
        "key": "agentesBiologicosRadio",
        "type": "radio",
        "templateOptions": {
          "label": "Hautatu aukera bat:",
          "required": true,
          "options": [
            {
              "value": "no",
              "label": "Ez"
            },
            {
              "value": "si",
              "label": "Bai"
            }
          ]
        }
      },
      {
        "template": "Erabiltzen den material biologiko mota",
        "hideExpression": "!model.agentesBiologicosRadio || model.agentesBiologicosRadio === 'no'"
      },
      {
        "type": "multicheckbox",
        "key": "muestrasBiologicasCheck",
        "hideExpression": "!model.agentesBiologicosRadio || model.agentesBiologicosRadio === 'no'",
        "templateOptions": {
          "label": "Aukerak:",
          "type": "array",
          "required": true,
          "options": [
            {
              "value": "criaymanejoanimales",
              "label": "Animalien hazkuntza eta erabilera"
            },
            {
              "value": "tejidossangrefluidosanimales",
              "label": "Animalien ehunak/odola/jariakinak"
            },
            {
              "value": "tejidossangrefluidoshumanos",
              "label": "Gizakien ehunak/odola/jariakinak"
            },
            {
              "value": "otros",
              "label": "Beste batzuk"
            }
          ]
        }
      },
      {
        "template": "Adierazi material biologiko mota",
        "hideExpression": "!((model.agentesBiologicosRadio && model.agentesBiologicosRadio === 'si') && (model.muestrasBiologicasCheck && model.muestrasBiologicasCheck.find((m) => m === 'otros')))"
      },
      {
        "key": "tipoMaterialBiologico",
        "type": "textarea",
        "hideExpression": "!((model.agentesBiologicosRadio && model.agentesBiologicosRadio === 'si') && (model.muestrasBiologicasCheck && model.muestrasBiologicasCheck.find((m) => m === 'otros')))",
        "templateOptions": {
          "label": "Adierazi:",
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      }
    ]
  }
]