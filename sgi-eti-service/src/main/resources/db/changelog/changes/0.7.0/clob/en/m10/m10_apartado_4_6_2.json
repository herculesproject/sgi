[
  {
    "key": "ap4_6_2",
    "wrappers": [
      "title-div"
    ],
    "templateOptions": {
      "expressionLock": "formState.memoria.tipoMemoria.id === 3",
      "label": "4.6.2 Training benefit for the student"
    },
    "hideExpression": "!formState.mainModel.ap4_6.estudiantesSujetosInvestigacionRadio || formState.mainModel.ap4_6.estudiantesSujetosInvestigacionRadio === 'no'",
    "fieldGroup": [
      {
        "template": "<p>Does participation as a research subject provide any educational benefit to the student?</p>"
      },
      {
        "key": "beneficioFormativoEstudianteRadio",
        "type": "radio",
        "templateOptions": {
          "required": true,
          "label": "Choose an option:",
          "options": [
            {
              "value": "no",
              "label": "No"
            },
            {
              "value": "si",
              "label": "Yes"
            }
          ]
        }
      },
      {
        "template": "Please specify",
        "hideExpression": "!model.beneficioFormativoEstudianteRadio || model.beneficioFormativoEstudianteRadio === 'no'"
      },
      {
        "key": "especificarBeneficioFormativo",
        "type": "textarea",
        "hideExpression": "!model.beneficioFormativoEstudianteRadio || model.beneficioFormativoEstudianteRadio === 'no'",
        "templateOptions": {
          "label": "Please specify",
          "placeholder": "Please specify",
          "required": true,
          "autosize": true,
          "autosizeMinRows": "1",
          "autosizeMaxRows": "10"
        }
      }
    ]
  }
]