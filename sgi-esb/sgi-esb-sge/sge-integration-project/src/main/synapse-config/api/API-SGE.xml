<?xml version="1.0" encoding="UTF-8"?>
<api context="/sge" name="API-SGE" publishSwagger="/_system/governance/swagger_files/sge-swagger.yaml" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="GET" url-mapping="/codigoeconomicos/gastos">
        <inSequence>
            <call-template target="conf:sequence-templates/find-all-in-sequence-template.xml">
                <with-param name="endpoint" value="sge-endpoint"/>
            </call-template>
        </inSequence>
        <outSequence>
            <sequence key="conf:sequences/find-all-out-sequence.xml"/>
        </outSequence>
        <faultSequence/>
    </resource>
    <resource methods="GET" url-mapping="/codigoeconomicos/ingresos">
        <inSequence>
            <call-template target="conf:sequence-templates/find-all-in-sequence-template.xml">
                <with-param name="endpoint" value="sge-endpoint"/>
            </call-template>
        </inSequence>
        <outSequence>
            <sequence key="conf:sequences/find-all-out-sequence.xml"/>
        </outSequence>
        <faultSequence/>
    </resource>
    <resource methods="GET" uri-template="/codigoeconomicos/{id}">
        <inSequence>
            <property expression="get-property('uri.var.id')" name="id" scope="default" type="STRING"/>
            <send>
                <endpoint key="sge-endpoint"/>
            </send>
        </inSequence>
        <outSequence>
            <sequence key="conf:sequences/find-one-out-sequence.xml"/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
