<?xml version="1.0" encoding="UTF-8"?>
<template name="save-in-sequence-template" xmlns="http://ws.apache.org/ns/synapse">
    <parameter isMandatory="true" name="endpoint"/>
    <sequence>
        <header name="Accept" scope="transport" value="application/json"/>
        <property expression="get-property('uri.var.id')" name="id" scope="default" type="STRING"/>
        <script description="TransformInputObjectMediator" key="conf:scripts/transform-input-object-mediator.js" language="js"/>
        <send>
            <endpoint key-expression="$func:endpoint"/>
        </send>
    </sequence>
</template>
